{
	"name": "Lucius Dark",
	"type": "dark",
	"colors": {
		"editor.background": "#303030",
		"editor.foreground": "#bcbcbc",
		"activityBarBadge.background": "#005f87",
		"statusBar.background": "#005f87",
		"sideBarTitle.foreground": "#bcbcbc",
		"terminal.background":"#303030",
		"terminal.foreground": "#bcbcbc",
		"terminal.ansiBlack": "#121212",
		"terminal.ansiBrightBlack": "#444444",
		"terminal.ansiRed": "#d75f5f",
		"terminal.ansiBrightRed": "#d75f5f",
		"terminal.ansiGreen": "#87af5f",
		"terminal.ansiBrightGreen": "#87af5f",
		"terminal.ansiYellow": "#afaf87",
		"terminal.ansiBrightYellow": "#afaf87",
		"terminal.ansiBlue": "#5fafd7",
		"terminal.ansiBrightBlue": "#5fafd7",
		"terminal.ansiMagenta": "#af87af",
		"terminal.ansiBrightMagenta": "#af87af",
		"terminal.ansiCyan": "#5faf87",
		"terminal.ansiBrightCyan": "#5faf87",
		"terminal.ansiWhite": "#626262",
		"terminal.ansiBrightWhite": "#bcbcbc",
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
			],
			"settings": {
				"foreground": "#6c6c6c"
			}
		},
		{
			"name": "Constant",
			"scope": [
				"constant",
				"string",
				"keyword.other.unit",
			],
			"settings": {
				"foreground": "#afaf87"
			}
		},
		{
			"name": "Normal Text",
			"scope": [
				"entity",
				"variable.parameter",
				"variable.name",
				"keyword.operator",
			],
			"settings": {
				"foreground": "#bcbcbc"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
			],
			"settings": {
				"foreground": "#d75f5f",
			}
		},
		{
			"name": "Keyword Control",
			"scope": [
				"keyword",
			],
			"settings": {
				"foreground": "#5fafd7",
			}
		},
		{
			"name": "Keyword Storage",
			"scope": [
				"storage",
				"variable.language",
				"meta.return-type",
			],
			"settings": {
				"foreground": "#5fafaf"
			}
		},
		{
			"name": "Support",
			"scope": [
				"support",
			],
			"settings": {
				"foreground": "#bcbcbc"
			}
		},
		{
			"name": "Preprocessor",
			"scope": [
				"macro",
				"meta.preprocessor",
				"keyword.control.directive.conditional.ifndef",
				"keyword.control.directive.conditional.ifdef",
				"keyword.control.directive.conditional.elif",
				"keyword.control.directive.conditional.else",
				"keyword.control.directive.conditional.endif",
				"keyword.control.directive.define",
				"keyword.control.directive.include",
				"keyword.control.directive.undef",
				"keyword.control.directive.error",
				"entity.name.function.preprocessor",
			],
			"settings": {
				"foreground": "#5faf87"
			}
		},

		{
			"name": "Special",
			"scope": [
				"markup.heading",
			],
			"settings": {
				"foreground": "#af87af"
			}
		},

		{
			"name": "Heading",
			"scope": [
				"entity.name.section",
			],
			"settings": {
				"foreground": "#5fafd7",
				"fontStyle": "bold",
			}
		},


	]
}
